<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<filter>
		<filter-name>LogFilter</filter-name>
		<filter-class>filter.Logfilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>LogFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<error-page>
		<error-code>404</error-code>
		<location>/exceptionNoPage.jsp</location>
	</error-page>
</web-app>